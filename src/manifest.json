
{
  "manifest_version": 2,

  "name": "Extension",
  "description": "Angular CLI Chrome app. ",
  "version": "1.0",

  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval' object-src 'self' https://connect.facebook.net/en_US/sdk.js;"

}